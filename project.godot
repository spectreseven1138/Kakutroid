[layer_names]

2d_physics/layer_1="World"
2d_physics/layer_2="Player"
2d_physics/layer_3="Player weapon"
2d_physics/layer_4="Enemy"
2d_physics/layer_5="Enemy weapon"
2d_physics/layer_20="World (fastfall bypass)"

[physics]

common/physics_fps=75
common/enable_pause_aware_picking=true
2d/default_gravity=300

[rendering]

2d/snapping/use_gpu_pixel_snap=true
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 0.211765, 0.239216, 0.298039, 1 )
quality/filters/use_fxaa=true
quality/filters/use_debanding=true
environment/default_environment="res://default_env.tres"
quality/dynamic_fonts/use_oversampling=false
