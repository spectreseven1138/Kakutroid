[application]

config/name="Kakutroid"
run/main_scene="res://src/scenes/rooms/test/Room.tscn"
config/icon="res://icon.png"

[autoload]

Utils="*res://addons/Godot-Utilities/Utils.gd"
InputManager="*res://src/scripts/singletons/InputManager.gd"
Enums="*res://src/scripts/singletons/Enums.gd"
Notification="*res://src/scripts/singletons/notification/NotificationDisplay.tscn"
Game="*res://src/scripts/singletons/Game.gd"
Overlay="*res://src/scripts/singletons/debug_overlay/overlay.gd"

[debug]

settings/fps/force_fps=75
gdscript/warnings/return_value_discarded=false
shapes/collision/max_contacts_displayed=0

[display]

window/size/width=1920
window/size/height=1080
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/Godot-Utilities/plugin.cfg", "res://addons/Godoxel/plugin.cfg", "res://addons/godot_sfxr/plugin.cfg", "res://addons/wakatime/plugin.cfg" )

